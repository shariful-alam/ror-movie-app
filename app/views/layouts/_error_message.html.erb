<div class="container-fluid" id="error_msg">
  <% if object.errors.full_messages.any? %>
    <% object.errors.full_messages.each do |error_message| %>
      <p class="alert alert-danger"> <%= error_message if object.errors.full_messages.first == error_message %> </p>
    <% break end %>
  <% end %>
</div>
